<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/nav_list">
    <fragment
        android:id="@+id/nav_list"
        android:name="com.reo.running.yumemitask.screen.ListFragment"
        android:label="ListFragment"
        tools:layout="@layout/fragment_list">
        <action
            android:id="@+id/action_nav_list_to_nav_details"
            app:destination="@id/nav_details" />
    </fragment>
    <fragment
        android:id="@+id/nav_details"
        android:name="com.reo.running.yumemitask.screen.DetailsFragment"
        android:label="DetailsFragment"
        tools:layout="@layout/fragment_details"/>
    <fragment
        android:id="@+id/nav_history"
        android:name="com.reo.running.yumemitask.screen.HistoryFragment"
        android:label="HistoryFragment"
        tools:layout="@layout/fragment_history"/>
</navigation>